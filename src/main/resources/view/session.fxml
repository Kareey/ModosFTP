<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<DialogPane fx:id="sessionDialog" prefHeight="91.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <content>
      <Pane prefHeight="26.0" prefWidth="360.0">
         <children>
            <Label fx:id="lbSessionName" layoutY="14.0" text="Session name:" />
            <TextField fx:id="tfSessionName" layoutX="107.0" layoutY="9.0" />
            <Button fx:id="btnConfirmSession" layoutX="298.0" layoutY="42.0" mnemonicParsing="false" onAction="#confirmSessionName" prefHeight="26.0" prefWidth="50.0" text="OK" />
         </children>
      </Pane>
   </content>
</DialogPane>
